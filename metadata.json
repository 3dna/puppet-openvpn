{
  "name": "puppet-openvpn",
  "version": "4.1.2-rc0",
  "author": "voxpupuli",
  "summary": "OpenVPN server puppet module",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-openvpn",
  "project_page": "https://github.com/voxpupuli/puppet-openvpn",
  "issues_url": "https://github.com/voxpupuli/puppet-openvpn/issues",
  "description": "Puppet module to manage OpenVPN servers",
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "15.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    },
    {
      "operatingsystem": "FreeBSD"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.0.1 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 < 5.0.0"
    }
  ],
  "tags": [
    "vpn",
    "openvpn"
  ]
}
